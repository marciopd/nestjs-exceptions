{
  "name": "nestjs-exceptions",
  "version": "7.6.7",
  "description": "Nest custom exceptions and exception filters.",
  "main": "dist/index",
  "typings": "dist/index",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf ./dist ./coverage",
    "tslint": "tslint -c tslint.json -t verbose 'spec/**/*.ts' 'src/**/*.ts'",
    "tslint:fix": "tslint --fix -c tslint.json -t verbose 'spec/**/*.ts' 'src/**/*.ts'",
    "ts": "tsc",
    "ts:watch": "tsc -w",
    "test": "jest --coverage",
    "build": "npm run clean; npm run ts",
    "prepublishOnly": "npm run build",
    "prets": "npm run clean; npm run tslint",
    "prets:watch": "npm run clean; npm run tslint:fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/marciopd/nestjs-exceptions.git"
  },
  "keywords": [
    "json",
    "nest",
    "exception",
    "error",
    "filter"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@nestjs/common": "^7.6.4",
    "@nestjs/core": "^7.6.4",
    "json-logger-service": "^7.6.4"
  },
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.16",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "eslint": "^7.16.0",
    "eslint-plugin-jsdoc": "^30.7.9",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "tslint": "^6.1.3",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "^4.1.3"
  }
}
